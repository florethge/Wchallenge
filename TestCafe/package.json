{
    "dependencies": {
        "dotenv": "^10.0.0",
        "testcafe": "^1.16.1",
        "testcafe-reporter-html": "^1.4.6"
    },
    "scripts": {
        "test-all-chrome-headless": "testcafe chrome:headless POM/tests/*.test.js ",
        "test-all-chrome": "testcafe chrome POM/tests/*.test.js ",
        "test-all-firefox": "testcafe firefox POM/tests/*.test.js ",
        "test-all-edge": "testcafe edge POM/tests/*.test.js ",
        "test-all-ie": "testcafe ie POM/tests/*.test.js ",
        "test-all-browsers": "testcafe all POM/tests/*.test.js ",
        "report-all-chrome": "testcafe chrome POM/tests/*.test.js --reporter html:report.html",
        "report-all-firefox": "testcafe firefox POM/tests/*.test.js --reporter html:report.html ",
        "report-all-edge": "testcafe edge POM/tests/*.test.js --reporter html:report.html ",
        "report-all-ie": "testcafe ie POM/tests/*.test.js --reporter html:report.html ",
        "report-all-browsers": "testcafe all POM/tests/*.test.js --reporter html:report.html "
    },
    "devDependencies": {
        "eslint": "^8.8.0",
        "eslint-plugin-testcafe": "^0.2.1"
    }
}
